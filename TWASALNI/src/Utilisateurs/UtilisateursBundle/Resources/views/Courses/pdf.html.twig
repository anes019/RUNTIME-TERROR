               </tr>
               </thead>
               <tbody id="myTable">
               {% for i in liste %}
                   <tr>

                       <td>{{i.id}}</td>
                       {#<td>{{ i.client.nom }}</td>#}
                       {#<td>{{ i.partenaire.nom }}</td>#}
                       <td>{{ i.course.depart }}</td>
                       <td>{{ i.course.destination }}</td>
                       <td>{{ i.pourcentage*100 }}</td>
                       <td>{{ i.course.prix }}</td>



                   </tr>
               {% endfor %}
               </tbody>
           </table>

