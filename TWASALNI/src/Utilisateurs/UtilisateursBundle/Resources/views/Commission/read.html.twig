<center><h1>Commission pour Mr {{ inventaire.partenaire.nom }} {{ inventaire.partenaire.prenom }}</h1></center>
<table border="1">
    <tr><th>Id</th><th>Depart</th><th>Destination</th><th>Prix de Course</th><th>Pourcentage</th></tr>
    {% for i in liste %}
        <tr>
            <td>{{ i.id }}</td>


            <td>{{ i.course.depart }}</td>
            <td>{{ i.course.destination }}</td>
            <td>{{ i.course.prix }}</td>
            <td>{{ i.pourcentage*100 }}%</td>



        </tr>

    {% endfor %}
</table>